APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

LOG_CHANNEL=stack
LOG_LEVEL=debug

# Database (adjust as needed)
DB_CONNECTION=sqlite
DB_DATABASE=database/database.sqlite

# Queue
QUEUE_CONNECTION=sync

# Session
SESSION_DRIVER=file
SESSION_LIFETIME=120

# Mail (placeholder)
MAIL_MAILER=smtp
MAIL_HOST=mailhog
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

# Gemini AI
GEMINI_API_KEY=
GEMINI_MODEL=gemini-1.5-flash-latest

# Weather
WEATHER_API_KEY=
WEATHER_DEFAULT_CITY=Jakarta,ID

# News
NEWS_API_KEY=
